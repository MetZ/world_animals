{
  "format_version": "1.21.0",
  "minecraft:entity": {
    "description": {
      "identifier": "worldanimals:rat",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false
    },
    "component_groups": {
      "minecraft:rat_baby": {
        "minecraft:is_baby": {},
        "minecraft:scale": {
          "value": 0.15
        },
        "minecraft:ageable": {
          "duration": 1200,
          "feed_items": [
            "minecraft:cheese",
            "minecraft:bread"
          ],
          "grow_up": {
            "event": "minecraft:ageable_grow_up",
            "target": "self"
          }
        },
        "minecraft:behavior.follow_parent": {
          "priority": 4,
          "speed_multiplier": 1.25
        }
      },
      "minecraft:rat_adult": {
        "minecraft:experience_reward": {
          "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
        },
        "minecraft:loot": {
          "table": "loot_tables/entities/rat.json"
        }
      },
      "minecraft:rat_wild": {
        "minecraft:tameable": {
          "probability": 1.0,
          "tame_items": "worldanimals:cheese",
          "tame_event": {
            "event": "minecraft:on_tame",
            "target": "self"
          }
        },
        "minecraft:on_target_acquired": {
          "event": "minecraft:become_angry",
          "target": "self"
        }
      },
      "minecraft:rat_tame": {
        "minecraft:is_tamed": {},
        "minecraft:health": {
          "value": 8,
          "max": 8
        },
        "minecraft:behavior.follow_owner": {
          "priority": 6,
          "speed_multiplier": 1.0,
          "start_distance": 10,
          "stop_distance": 2
        },
        "minecraft:attack": {
          "damage": 2
        },
        "minecraft:behavior.owner_hurt_by_target": {
          "priority": 1
        },
        "minecraft:behavior.owner_hurt_target": {
          "priority": 2
        },
        "minecraft:sittable": {},
        "minecraft:leashable": {
          "soft_distance": 3.0,
          "hard_distance": 5.0,
          "max_distance": 8.0,
          "on_leash": {
            "event": "minecraft:on_leash",
            "target": "self"
          },
          "on_unleash": {
            "event": "minecraft:on_unleash",
            "target": "self"
          }
        }
      },
      "minecraft:rat_angry": {
        "minecraft:attack": {
          "damage": 3
        },
        "minecraft:angry": {
          "duration": 400,
          "broadcast_anger": false,
          "broadcast_range": 15,
          "calm_event": {
            "event": "minecraft:on_calm",
            "target": "self"
          }
        }
      }
    },
    "components": {
      "minecraft:nameable": {},
      "minecraft:type_family": {
        "family": [
          "rat",
          "mob"
        ]
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": 0,
        "breathes_air": true,
        "breathes_water": true
      },
      "minecraft:collision_box": {
        "width": 0.5,
        "height": 0.35
      },
      "minecraft:navigation.generic": {
        "is_amphibious": true,
        "can_path_over_water": true,
        "can_swim": true,
        "can_walk": true,
        "can_breach": false,
        "can_jump": false
      },
      "minecraft:underwater_movement": {
        "value": 0.15
      },
      "minecraft:health": {
        "value": 8,
        "max": 8
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": {
              "test": "in_lava",
              "subject": "self",
              "operator": "==",
              "value": true
            },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:movement": {
        "value": 0.25
      },
      "minecraft:movement.basic": {},
      "minecraft:jump.static": {},
      "minecraft:can_climb": {},
      "minecraft:attack": {
        "damage": 3
      },
      "minecraft:healable": {
        "items": [
          {
            "item": "minecraft:cheese",
            "heal_amount": 3
          },
          {
            "item": "minecraft:bread",
            "heal_amount": 5
          }
        ]
      },
      "minecraft:behavior.float": {
        "priority": 0
      },
      "minecraft:behavior.random_stroll": {
        "priority": 8,
        "speed_multiplier": 1.0
      },
      "minecraft:behavior.look_at_player": {
        "priority": 6,
        "target_distance": 6.0,
        "probability": 0.02
      },
      "minecraft:behavior.beg": {
        "priority": 9,
        "look_distance": 8,
        "look_time": [
          2,
          4
        ],
        "items": [
          "minecraft:cheese",
          "minecraft:bread"
        ]
      },
      "minecraft:behavior.hurt_by_target": {
        "priority": 3
      },
      "minecraft:physics": {},
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:damage_sensor": {
        "triggers": {
          "on_damage": {
            "event": "react",
            "deals_damage": true
          }
        }
      }
    },
    "events": {
      "minecraft:entity_spawned": {
        "randomize": [
          {
            "weight": 90,
            "add": {
              "component_groups": [
                "minecraft:rat_adult",
                "minecraft:rat_wild"
              ]
            }
          },
          {
            "weight": 10,
            "add": {
              "component_groups": ["minecraft:rat_baby"]
            }
          }
        ]
      },
      "minecraft:entity_born": {
        "add": {
          "component_groups": [
            "minecraft:rat_baby",
            "minecraft:rat_tame"
          ]
        }
      },
      "minecraft:ageable_grow_up": {
        "remove": {
          "component_groups": ["minecraft:rat_baby"]
        },
        "add": {
          "component_groups": ["minecraft:rat_adult"]
        }
      },
      "minecraft:ageable_set_baby": {
        "remove": {
          "component_groups": ["minecraft:rat_adult"]
        },
        "add": {
          "component_groups": ["minecraft:rat_baby"]
        }
      },
      "minecraft:on_tame": {
        "remove": {
          "component_groups": ["minecraft:rat_wild"]
        },
        "add": {
          "component_groups": ["minecraft:rat_tame"]
        }
      },
      "minecraft:become_angry": {
        "remove": {
          "component_groups": ["minecraft:rat_wild"]
        },
        "add": {
          "component_groups": ["minecraft:rat_angry"]
        }
      },
      "minecraft:on_calm": {
        "remove": {
          "component_groups": ["minecraft:rat_angry"]
        },
        "add": {
          "component_groups": ["minecraft:rat_wild"]
        }
      }
    }
  }
}
